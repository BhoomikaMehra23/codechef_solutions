#Problem: LUCKYNUM
#include <stdio.h>

int main(void) {
    int t,i;
    scanf("%d",&t);
    for(i=0;i<t;i++)
    {
        int a,b,c;
        scanf("%d%d%d",&a,&b,&c);
        if(a==7 || b==7 ||c==7)
        {
            printf("YES\n");
            
        }
        else
        printf("NO\n");
    }
	// your code goes here
	return 0;
}

